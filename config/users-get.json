{
    "request": {
        "method": "GET",
        "path": "/users/*"
    },
	"response": {
        "statusCode": 200,
        "headers": {
			"Content-Type":["application/json"]
		},
		"persisted": {
            "name" : "users/user-{{request.url./users/(?P<value>\\d+)}}",
            "notFound": {
                "statusCode": 404,
                "body": "Record user-{{request.url./users/(?P<value>\\d+)}} in collection users Not Found"
            }
        }
    }
}